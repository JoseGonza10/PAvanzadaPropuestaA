    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h1 class="gallery_taital">Nuestras mejores ofertas</h1>
            </div>
        </div>
        <div class="gallery_section_2">
            <div class="row">
                @if(Model != null)
                {
                    int count = 3;
                    foreach(Design item in Model)
                    {
                        @await Html.PartialAsync("_diseno", item)
                        <br>
                    }
                }
            </div>
        </div>
    </div>
</div>
<!-- gallery section end -->
